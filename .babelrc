{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": "commonjs"
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "absoluteRuntime": false,
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ],
    [
      "babel-plugin-react-css-modules",
      {
        "context": "./src",
        "generateScopedName": "[path][name]__[local]",
        "handleMissingStyleName": "throw",
        "webpackHotModuleReloading": true
      }
    ]
  ]
}